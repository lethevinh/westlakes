<div class="list_giai_keo row">
    {% for category in categories %}
    <div class="col-md-4 col-xs-12">
        <div class="sk-item">
            <a target="_blank" class="newWindow"
                                data-href="{{category.link}}"
                                href="{{category.link}}" role="tab">
                <img class="lazy"
                     data-src="{{category.meta('category_icon').url}}"
                        src="{{ theme_url() }}/assets/images/dot.jpg">
                {{category.title}}
            </a>
        </div>
    </div>
    {% endfor %}
</div>