{
  "name": "vi-theme-default",
  "version": "1.0.0",
  "description": "The \"_s\" for Timber: a dead-simple theme that you can build from. The primary purpose of this theme is to provide a file structure rather than a framework for markup or styles. Configure your Sass, scripts, and task runners however you would like!",
  "main": "resources/scripts/theme.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "animsition": "^4.0.2",
    "bootstrap": "^4.3.1",
    "easy-pie-chart": "^2.1.7",
    "flexslider": "^2.7.2",
    "imagesloaded": "^4.1.4",
    "infinite-scroll": "^3.0.6",
    "instafeed.js": "^1.4.1",
    "isotope-layout": "^3.0.6",
    "jquery": "^3.4.1",
    "jquery-bridget": "^2.0.1",
    "jquery-countto": "^1.2.0",
    "jquery-form": "^4.2.2",
    "jquery-ui": "^1.12.1",
    "jquery-validation": "^1.19.1",
    "jquery.mb.ytplayer": "^3.2.10",
    "jrespond": "^1.0.0",
    "jribbble": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lazyload": "^2.0.0-rc.2",
    "magnific-popup": "^1.1.0",
    "owl.carousel": "^2.3.4",
    "popper.js": "^1.16.0",
    "skrollr": "^0.6.26",
    "superfish": "^1.7.10",
    "sweetalert": "^2.1.2",
    "swiper": "^5.2.0",
    "theia-sticky-sidebar": "^1.7.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "json-loader": "^0.5.7",
    "laravel-mix": "^5.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.23.6",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lethevinh/vi-theme-default.git"
  },
  "author": "VinhLe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lethevinh/vi-theme-default/issues"
  },
  "homepage": "https://github.com/lethevinh/vi-theme-default#readme"
}
